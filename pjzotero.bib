@article{BurfurdWilkening2018,
	title = {Experimental guidance for eliciting beliefs with the {Stochastic} {Becker}–{DeGroot}–{Marschak} mechanism},
	volume = {4},
	issn = {2199-6784},
	url = {https://doi.org/10.1007/s40881-018-0046-5},
