@article{HaghaniEtAl2021,
  title = {Hypothetical Bias in Stated Choice Experiments: {{Part I}}. {{Macro-scale}} Analysis of Literature and Integrative Synthesis of Empirical Evidence from Applied Economics, Experimental Psychology and Neuroimaging},
  shorttitle = {Hypothetical Bias in Stated Choice Experiments},
  author = {Haghani, Milad and Bliemer, Michiel C. J. and Rose, John M. and Oppewal, Harmen and Lancsar, Emily},
  year = {2021},
