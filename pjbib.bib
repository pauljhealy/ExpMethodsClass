@incollection{HarrisonSwarthout2023,
  title = {Cumulative {{Prospect Theory}} in the {{Laboratory}}: {{A Reconsideration}}},
  shorttitle = {Cumulative {{Prospect Theory}} in the {{Laboratory}}},
  booktitle = {Models of {{Risk Preferences}}: {{Descriptive}} and {{Normative Challenges}}},
  author = {Harrison, Glenn W. and Swarthout, J. Todd},
